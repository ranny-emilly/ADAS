//p incluir biblioteca
#include <bits/stdc++.h>
using namespace std;

int main () {

    //declarar a variável "a" como inteira
    int e;  

    //funcao de leia
    cin >> e;
    
   
   if (e%4 != 0) cout << "365";
   else if (e%4 == 0 && e%100 != 0) cout << "366";
   else if (e%100 == 0 && e%400 != 0) cout << "365";
   else if (e%400 == 0) cout << "366";

    //funcao de saída     

return 0;

}
