#include <bits/stdc++.h>

using namespace std;

int main() {
    
    int dias, acessd, i, soma;
    
    cin >> dias;
    soma = 0;
    
    for (i = 0; i < dias; i++){
        cin >> acessd;
        soma += acessd;
        
    if (soma >= 1000000){
           break;
       }
    }
    
    cout << (i + 1);
  return 0;
}
