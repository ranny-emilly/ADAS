#include <bits/stdc++.h>

using namespace std;

int main(){
    
   int p1,p2, c1, c2, result, result2;
   
   cin >> p1 >> c1 >> p2 >> c2;
   
   
   result = p1 * c1;
    result2 = p2 * c2;
  
  if (result > result2){
      cout << "-1";
  } else if (result < result2){
      cout << "1";
      
  } else if (result == result2){
      cout << "0";
  }
  
   
    
    return 0;
}
